{
  "version": 2,
  "builds": [
    { "src": "api/notify.js", "use": "@vercel/node" },
    { "src": "api/notifications.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/notify", "dest": "/api/notify.js" },
    { "src": "/api/notifications", "dest": "/api/notifications.js" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
